{
  "name": "louvr3d",
  "version": "1.0.0",
  "description": "3D art exhibit",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevans0625/LouVR3D.git"
  },
  "author": "LouVR3D",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevans0625/LouVR3D/issues"
  },
  "homepage": "https://github.com/kevans0625/LouVR3D#readme",
  "dependencies": {
    "aframe": "^1.0.4",
    "aframe-environment-component": "^2.0.0",
    "aframe-layout-component": "^5.3.0",
    "aframe-react": "^4.4.0",
    "add": "^2.0.6",
    "aws-sdk": "^2.683.0",
    "axios": "^0.18.0",
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "connect-mongo": "^3.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.9.18",
    "mongoose-sequence": "^5.2.2",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "superagent": "^5.2.2",
    "uuidv4": "^6.0.9"
  },
